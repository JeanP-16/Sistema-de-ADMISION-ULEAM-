# ==========================================
# ARCHIVO: .env
# Configuración del servidor TypeScript
# ==========================================

# Puerto del servidor TypeScript
PORT=3000

# URL del servidor Flask (para CORS)
FLASK_URL=http://localhost:5000

# Entorno de ejecución
NODE_ENV=development

# Base de datos (futuro)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=admision_uleam
# DB_USER=postgres
# DB_PASSWORD=

# JWT Secret (futuro, para autenticación)
# JWT_SECRET=tu_clave_secreta_aqui

# Logs
LOG_LEVEL=info